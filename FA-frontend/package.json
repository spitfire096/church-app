{
    "name": "fa-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3000",
        "build": "next build",
        "start": "next start -p 3000",
        "lint": "eslint . --ext .ts,.tsx",
        "migrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
        "migrate:undo": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo",
        "migrate:undo:all": "cross-env NODE_ENV=development npx sequelize-cli db:migrate:undo:all",
        "migrate:create": "sequelize-cli migration:create",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ci": "jest --ci --coverage --maxWorkers=2 --testResultsProcessor=jest-sonar-reporter"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.18",
        "@heroicons/react": "^2.1.1",
        "@tailwindcss/forms": "^0.5.10",
        "axios": "^1.6.7",
        "chart.js": "^4.4.7",
        "next": "^15.1.6",
        "react": "^19.0.0",
        "react-chartjs-2": "^5.3.0",
        "react-csv": "^2.2.2",
        "react-dom": "^19.0.0",
        "next-auth": "^4.24.6"
    },
    "devDependencies": {
        "@types/node": "^20.11.16",
        "@types/react": "^18.2.52",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "6.20.0",
        "@typescript-eslint/parser": "6.20.0",
        "autoprefixer": "^10.4.17",
        "eslint-config-next": "14.2.23",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.3.3"
    },
    "overrides": {
        "@babel/traverse": "^7.23.2",
        "semver": "^7.5.3",
        "word-wrap": "^1.2.4",
        "@adobe/css-tools": "^4.3.1",
        "follow-redirects": "^1.15.4",
        "axios": {
            "follow-redirects": "^1.15.4"
        }
    },
    "jestSonar": {
        "reportPath": ".",
        "reportFile": "test-report.xml",
        "indent": 4
    }
}
